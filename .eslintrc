{
    "env": {
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "ecmaFeatures": {
            "globalReturn ": true
        }
    },
    "extends": "airbnb-base",
    "rules": {
        "indent": ["error", 4],
        "no-var": 0,
        "object-shorthand": 0,
        "arrow-body-style": 0,
        "comma-dangle": 0,
        "no-console": 0,
        "no-plusplus": 0,
        "max-len": [ "error", 140 ],
        "no-param-reassign": 0,
        "import/no-unresolved": [ 0, {"commonjs": true, "amd":true} ]
    }
}
